    {
      "parameters": {
        "mode": "runOnceForEachItem",
        "jsCode": "const input = $input.item.json;\n\nconst weights = {\n  budget: 30,\n  niche: 25,\n  experience: 20,\n  adHistory: 15,\n  urgency: 10,\n};\n\nlet score = 0;\n\n// Extract responses\nconst budgetRaw = input[\"Whatâ€™s your average monthly marketing budget?\"];\nconst niche = input[\"Business Niche\"]?.toLowerCase() || '';\nconst experience = input[\"Main Business Goal\"]?.toLowerCase() || '';\nconst hasRunAds = input[\"Have you run ads before?\"] === true;\nconst urgency = input[\"Do you have video content or testimonials ready?\"] === true;\n\n// --- Budget scoring\nconst budget = parseFloat(budgetRaw?.replace(/[^0-9.]/g, '') || '0');\nif (budget >= 3000) score += weights.budget;\nelse if (budget >= 1000) score += weights.budget * 0.6;\nelse score += weights.budget * 0.3;\n\n// --- Niche scoring\nconst highValueNiches = ['real estate', 'ecommerce', 'coaching', 'saas'];\nif (highValueNiches.includes(niche)) score += weights.niche;\nelse score += weights.niche * 0.5;\n\n// --- Experience (based on goal)\nif (experience.includes('scale')) score += weights.experience;\nelse score += weights.experience * 0.5;\n\n// --- Ad History\nscore += hasRunAds ? weights.adHistory : weights.adHistory * 0.2;\n\n// --- Urgency\nscore += urgency ? weights.urgency : weights.urgency * 0.5;\n\nreturn {\n  json: {\n    ...input,\n    leadScore: Math.round(score),\n  }\n};\n"
      },
